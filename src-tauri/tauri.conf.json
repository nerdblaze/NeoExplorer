{
  "productName": "neoexplorer",
  "version": "0.1.0",
  "identifier": "in.bongbaba.napp.explorer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:2378",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "NeoExplorer",
        "width": 800,
        "height": 600,
        "resizable": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": ["msi"],
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}